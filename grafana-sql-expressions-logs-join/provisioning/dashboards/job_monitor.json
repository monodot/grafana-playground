  "links": [],
  "panels": [
    {
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "description": "Shows recent jobs and their start and finish times. It uses two Loki queries to find job starts and ends, and then joins them together by job_id using a SQL Expression.",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "align": "auto",
            "cellOptions": {
              "type": "auto"
            },
            "footer": {
              "reducers": []
            },
            "inspect": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "red",
                "value": 80
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Status"
            },
            "properties": [
              {
                "id": "custom.cellOptions",
                "value": {
                  "type": "pill"
                }
              },
              {
                "id": "mappings",
                "value": [
                  {
                    "options": {
                      "error": {
                        "color": "red",
                        "index": 1
                      },
                      "success": {
                        "color": "green",
                        "index": 0
                      }
                    },
                    "type": "value"
                  }
                ]
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Job ID"
            },
            "properties": []
          }
        ]
      },
      "gridPos": {
        "h": 14,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "cellHeight": "sm",
        "showHeader": true
      },
      "pluginVersion": "12.3.1",
      "targets": [
        {
          "datasource": {
            "type": "loki",
            "uid": "loki"
          },
          "direction": "backward",
          "editorMode": "code",
          "expr": "{service_name=\"job-simulator\"} | logfmt | event=`start`",
          "hide": true,
          "queryType": "range",
          "refId": "job_starts"
        },
        {
          "datasource": {
            "type": "loki",
            "uid": "loki"
          },
          "direction": "backward",
          "editorMode": "code",
          "expr": "{service_name=\"job-simulator\"} | logfmt | event=`end`",
          "hide": true,
          "queryType": "range",
          "refId": "job_completions"
        },
        {
          "datasource": {
            "name": "Expression",
            "type": "__expr__",
            "uid": "__expr__"
          },
          "expression": "SELECT\n  JSON_EXTRACT(job_starts.labels, '$.job_id') AS 'Job ID',\n  JSON_EXTRACT(job_completions.labels, '$.status') AS 'Status',\n  job_starts.Time as 'Start time',\n  COALESCE(job_completions.Time, '(in progress)') AS 'End time'\nFROM\n  job_starts\n  LEFT OUTER JOIN job_completions ON JSON_EXTRACT(job_starts.labels, '$.job_id') = JSON_EXTRACT(job_completions.labels, '$.job_id') --  JSON_EXTRACT(starts.labels, '$.brokkr_owner') AS 'Owner',\nORDER BY job_starts.Time DESC\nLIMIT\n  50",
          "hide": false,
          "refId": "A",
          "type": "sql"
        }
      ],
      "title": "Recent jobs",
      "type": "table"
    }
  ],
  "preload": false,
